<style>
    .parent {
        display: grid;
        grid-template-columns: repeat(9, 1fr);
        grid-template-rows: repeat(5, 1fr);
        grid-column-gap: 0px;
        grid-row-gap: 0px;
    }

    .div1 {
        grid-area: 1 / 3 / 6 / 8;
        background-color: red;
    }

    .div2 {
        grid-area: 1 / 1 / 6 / 3;
        background-color: black;
        display: flex;
        justify-content: center;
        align-items: center;
    }

    .div3 {
        grid-area: 1 / 8 / 6 / 10;
        background-color: black;
        display: flex;
        justify-content: center;
        align-items: center;
    }

        .div2 img,
        .div3 img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }
</style>

<div class="parent">
    <div class="div1"></div>
    <div class="div2"> <img src="@Url.Content("/staticImages/sideimgleft.png")" /> </div>
    <div class="div3"> <img src="@Url.Content("/staticImages/sideimgright.png")" /> </div>
</div>
